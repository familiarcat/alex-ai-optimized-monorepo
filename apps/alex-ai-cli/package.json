{
  "name": "alex-ai-cli",
  "version": "1.0.0",
  "description": "Alex AI Universal CLI - Cross-Platform Development with Dynamic Framework Evolution",
  "main": "dist/index.js",
  "bin": {
    "alex": "./dist/index.js"
  },
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "create": "tsx src/commands/create.ts",
    "evolve": "tsx src/commands/evolve.ts",
    "sync": "tsx src/commands/sync.ts",
    "translate": "tsx src/commands/translate.ts"
  },
  "dependencies": {
    "@alex-ai/core": "workspace:*",
    "@alex-ai/universal": "workspace:*",
    "@oclif/core": "^3.0.0",
    "@oclif/plugin-help": "^5.0.0",
    "commander": "^11.0.0",
    "inquirer": "^9.0.0",
    "chalk": "^5.0.0",
    "ora": "^7.0.0",
    "figlet": "^1.6.0",
    "axios": "^1.6.0",
    "openai": "^5.19.1",
    "dotenv": "^17.2.2",
    "fs-extra": "^11.0.0",
    "glob": "^10.0.0",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "execa": "^8.0.0",
    "semver": "^7.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/inquirer": "^9.0.0",
    "@types/fs-extra": "^11.0.0",
    "@types/lodash": "^4.17.0",
    "@types/uuid": "^9.0.0",
    "@types/figlet": "^1.5.0",
    "typescript": "^5.0.0",
    "tsx": "^4.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  },
  "keywords": [
    "alex-ai",
    "cli",
    "cross-platform",
    "react",
    "nextjs",
    "react-native",
    "universal",
    "ai-powered",
    "framework-evolution"
