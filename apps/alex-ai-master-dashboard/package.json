{
  "name": "alex-ai-master-dashboard",
  "version": "1.0.0",
  "private": true,
  "description": "Alex AI Master Dashboard - Unified entry point to all specialized applications",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate": "supabase gen types typescript --project-id rpkkkbufdwxmjaerbhbn > src/types/supabase.ts",
    "db:push": "supabase db push",
    "db:reset": "supabase db reset",
    "db:seed": "supabase db seed",
    "deploy:preview": "vercel --target preview",
    "deploy:production": "vercel --target production",
    "dashboard:test": "node scripts/test-dashboard-integration.js",
    "apps:status": "node scripts/check-apps-status.js"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
