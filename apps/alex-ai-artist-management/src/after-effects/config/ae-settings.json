{
  "projectSettings": {
    "name": "AlexAI_Animations",
    "version": "2024",
    "frameRate": 60,
    "duration": 3.0,
    "width": 1920,
    "height": 1080,
    "pixelAspectRatio": 1.0
  },
  "exportSettings": {
    "format": "lottie",
    "version": "5.7.4",
    "quality": "high",
    "optimize": true,
    "includeAssets": true,
    "trimComp": true
  },
  "colorPalette": {
    "primary": "#33CCFF",
    "secondary": "#CC33FF", 
    "accent": "#FF9900",
    "success": "#33FF66",
    "warning": "#FFCC00",
    "error": "#FF3366",
    "background": "#0D0D0D",
    "surface": "#262626",
    "text": "#F2F2F2"
  },
  "animationPresets": {
    "cta": {
      "duration": 2.0,
      "easing": "easeOutCubic",
      "loop": false
    },
    "loading": {
      "duration": 1.5,
      "easing": "easeInOutCubic", 
      "loop": true
    },
    "success": {
      "duration": 1.0,
      "easing": "easeOutBack",
      "loop": false
    },
    "error": {
      "duration": 1.0,
      "easing": "easeOutBounce",
      "loop": false
    },
    "hover": {
      "duration": 0.3,
      "easing": "easeOutCubic",
      "loop": false
    },
    "scroll": {
      "duration": 2.0,
      "easing": "easeInOutSine",
      "loop": true
    }
  },
  "responsiveBreakpoints": {
    "mobile": {
      "width": 375,
      "height": 667,
      "scale": 0.5
    },
    "tablet": {
      "width": 768,
      "height": 1024,
      "scale": 0.75
    },
    "desktop": {
      "width": 1920,
      "height": 1080,
      "scale": 1.0
    }
  }
}
